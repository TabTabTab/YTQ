	public static ArrayList<Harbor> findNeighbours(Game game, Harbor current){
		ArrayList<Harbor> neighbours = new ArrayList<Harbor>();
		for(Route route: game.getRouteList()){
			if(route.getFromHarborId() == current.getId()){
				neighbours.add(game.getHarborByID(route.getToHarborId()));
			}else if(route.getToHarborId() == current.getId()){
				neighbours.add(game.getHarborByID(route.getFromHarborId()));
			}
		}
		return neighbours;
		
	}
